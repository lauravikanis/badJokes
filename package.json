{
  "name": "bad_jokes",
  "private": true,
  "version": "1.0.0",
  "description": "a page fo your favourite bad jokes",
  "main": "index.js",
  "dependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^5.3.0",
    "express": "^4.17.3",
    "immer": "^8.0.1",
    "init": "^0.1.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "postinstall": "^0.7.0",
    "prettier": "^2.2.0",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1"
  },
  "scripts": {
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build && npm run build-storybook",
    "test:watch": "cd client && npm run test:watch",
    "test": "npm run lint && npm run prettier && cd client && npm test",
    "lint": "eslint . --ext .js",
    "prettify": "prettier --write .",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,html,css,json}\"",
    "storybook": "cd client && npm run storybook",
    "server": "nodemon server.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lauravikanis/badJokes.git"
  },
  "author": "Laura Vikanis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lauravikanis/badJokes/issues"
  },
  "homepage": "https://github.com/lauravikanis/badJokes#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
